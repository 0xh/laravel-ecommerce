{
    "name": "ablunier/laravel-ecommerce",
    "description": "",
    "authors": [
        {
            "name": "Adrian P. Blunier",
            "email": "adrian@blunier.net"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.0.*",
        "illuminate/database": "5.0.*",
        "dimsav/laravel-translatable": "5.1"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "0.9.*",
        "phpspec/phpspec": "~2.1"
    },
    "config": {
        "bin-dir": "bin/"
    },
    "autoload": {
        "classmap": [
            "migrations",
            "tests"
        ],
        "psr-4": {
            "ANavallaSuiza\\Ecommerce\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ANavallaSuiza\\PhpSpec\\": "spec/ANavallaSuiza/PhpSpec",
            "ANavallaSuiza\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev"
}
